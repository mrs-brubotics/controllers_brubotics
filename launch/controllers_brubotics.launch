<launch>
  <--! Parameter definition >
  <arg name="RUN_TYPE" default="$(env RUN_TYPE)" />
  <arg name="UAV_TYPE" default="$(env UAV_TYPE)" />
  <arg name="custom_config_so3_controller_interns" default="" />
  <arg name="PROFILER" default="$(optenv PROFILER false)" />

  <rosparam ns="$(env UAV_NAME)/control_manager/so3_controller_interns" file="$(find controllers_brubotics)/config/default/so3_interns.yaml" />
  <rosparam ns="$(env UAV_NAME)/control_manager/so3_controller_interns" file="$(find controllers_brubotics)/config/$(arg RUN_TYPE)/$(arg UAV_TYPE)/so3_interns.yaml" />
  <rosparam if="$(eval not arg('custom_config_so3_controller_interns') == '')" ns="$(env UAV_NAME)/control_manager/so3_controller_interns" file="$(arg custom_config_so3_controller_interns)" />
  <param name="$(env UAV_NAME)/control_manager/so3_controller_interns/enable_profiler" type="bool" value="$(arg PROFILER)" />
  <remap from="~$(env UAV_NAME)/control_manager/so3_controller_interns/profiler" to="profiler" />

  <rosparam ns="$(env UAV_NAME)/control_manager" file="$(find controllers_brubotics)/config/address.yaml" />


</launch>
